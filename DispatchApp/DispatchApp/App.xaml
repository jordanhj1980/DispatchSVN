<Application x:Class="DispatchApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
         
    	<ResourceDictionary>
    		<ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Generic.xaml"/>
                <ResourceDictionary Source="/DispatchApp;component/Client/UserlabelResourceDictionary.xaml"/>
                <ResourceDictionary Source="NewPanelDictionary.xaml"/>
                <ResourceDictionary Source="/DispatchApp;component/Server/ManageStyleResource.xaml"/>
                <!--  样式分离 不用的可以不引用 减少内存暂用  -->

                <!--  DMSKin内置转换器 配色  -->
                <ResourceDictionary Source="pack://application:,,,/USERSkin;Component/Styles/USERSkin.xaml" />
                <!--  DMSKin内置按钮  -->
                <ResourceDictionary Source="pack://application:,,,/USERSkin;component/Styles/USERButton.xaml" />
                <!--  DMSKin内置SVG图标  -->
                <ResourceDictionary Source="pack://application:,,,/USERSkin;component/Styles/USERIcon.xaml" />
            </ResourceDictionary.MergedDictionaries>
    	</ResourceDictionary>
    </Application.Resources>
</Application>
