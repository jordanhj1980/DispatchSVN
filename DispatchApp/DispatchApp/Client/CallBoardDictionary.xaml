<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Style x:Key="CallBoardButton" TargetType="Button">
        <Setter Property="Background" Value="{x:Null}"></Setter>
        <Setter Property="Foreground" Value="#FFD8D7D7"></Setter>
        <Setter Property="FontFamily" Value="SimHei"></Setter>
        <Setter Property="Height" Value="30"></Setter>
        <Setter Property="Width" Value="65"></Setter>
        <Setter Property="FontSize" Value="24"></Setter> 
    </Style>
    
    <Style x:Key="CallBoardKry" TargetType="Grid">
        <Setter Property="Height" Value="295"></Setter>
        <Setter Property="Width" Value="305"></Setter>
    </Style>
</ResourceDictionary>